<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" width="144" height="144" viewBox="0 0 144 144">
  <!-- Neural Savanna PWA Icon -->
  <defs>
    <radialGradient id="bg-gradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#000000;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1a1a1a;stop-opacity:1" />
    </radialGradient>
    <radialGradient id="tree-gradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#228B22;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#32CD32;stop-opacity:1" />
    </radialGradient>
    <radialGradient id="neural-glow" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#4169E1;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#32CD32;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#90EE90;stop-opacity:0" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="144" height="144" fill="url(#bg-gradient)" rx="20"/>
  
  <!-- Savanna background -->
  <rect x="0" y="100" width="144" height="44" fill="#D2B48C"/>
  <circle cx="120" cy="30" r="25" fill="#FFD700" opacity="0.8"/>
  
  <!-- Acacia tree (main symbol) -->
  <ellipse cx="72" cy="85" rx="8" ry="25" fill="#8B4513"/>
  <circle cx="72" cy="65" r="20" fill="url(#tree-gradient)"/>
  <circle cx="85" cy="55" r="15" fill="url(#tree-gradient)"/>
  <circle cx="60" cy="60" r="12" fill="url(#tree-gradient)"/>
  
  <!-- Neural network nodes -->
  <circle cx="40" cy="40" r="6" fill="url(#neural-glow)"/>
  <circle cx="104" cy="50" r="6" fill="url(#neural-glow)"/>
  <circle cx="50" cy="110" r="6" fill="url(#neural-glow)"/>
  <circle cx="94" cy="100" r="6" fill="url(#neural-glow)"/>
  
  <!-- Connection lines -->
  <line x1="40" y1="40" x2="72" y2="65" stroke="#32CD32" stroke-width="2" opacity="0.7"/>
  <line x1="104" y1="50" x2="72" y2="65" stroke="#32CD32" stroke-width="2" opacity="0.7"/>
  <line x1="50" y1="110" x2="72" y2="85" stroke="#32CD32" stroke-width="2" opacity="0.7"/>
  <line x1="94" y1="100" x2="72" y2="85" stroke="#32CD32" stroke-width="2" opacity="0.7"/>
  
  <!-- Ubuntu symbol -->
  <circle cx="72" cy="72" r="8" fill="none" stroke="#FFD700" stroke-width="2"/>
  <path d="M68 68 L76 76 M68 76 L76 68" stroke="#FFD700" stroke-width="1.5"/>
</svg> 